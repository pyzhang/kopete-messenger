	QMap<QString, int> sortedMap( reverseMap( messengerProtocol->countries() ) );
	QMapIterator<QString, int> it( sortedMap );

	while ( it.hasNext() )
	{
		it.next();
		m_genInfoWidget->mobileCountryComboBox->addItem( it.key(), it.value() );
	}

